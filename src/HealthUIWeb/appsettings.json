{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "KickStarter API",
        "Uri": "https://localhost:7270/health-diag"
      },
      {
        "Name": "KickStarter Web",
        "Uri": "https://localhost:7191/health-diag"
      }
    ],
    "EvaluationTimeInSeconds": 60,
    "MinimumSecondsBetweenFailureNotifications": 360
  }
}
